{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "linebreak-style": 0,
      "max-len": ["error", { "code": 130 }],
      "new-cap": 0,
      "require-jsdoc" : 0
    }
}
